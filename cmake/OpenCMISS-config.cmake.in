@PACKAGE_INIT@

set(OpenCMISS_MAJOR_VERSION "@OpenCMISS_MAJOR_VERSION@")
set(OpenCMISS_MINOR_VERSION "@OpenCMISS_MINOR_VERSION@")
set(OpenCMISS_PATCH_VERSION "@OpenCMISS_PATCH_VERSION@")

set(OpenCMISS_WITH_SINGLE_PRECISION @OpenCMISS_WITH_SINGLE_PRECISION@)
set(OpenCMISS_WITH_LARGE_INDICES @OpenCMISS_WITH_LARGE_INDICES@)

set(OpenCMISS_WITH_MPI @OpenCMISS_WITH_MPI@)
set(OpenCMISS_WITH_OpenMP @OpenCMISS_WITH_OpenMP@)
set(OpenCMISS_WITH_CUDA @OpenCMISS_WITH_CUDA@)

set(OpenCMISS_WITH_CellML @OpenCMISS_WITH_CellML@)
set(OpenCMISS_WITH_FieldML @OpenCMISS_WITH_FieldML@)
set(OpenCMISS_WITH_HYPRE @OpenCMISS_WITH_HYPRE@)
set(OpenCMISS_WITH_MUMPS @OpenCMISS_WITH_MUMPS@)
set(OpenCMISS_WITH_ParMETIS @OpenCMISS_WITH_ParMETIS@)
set(OpenCMISS_WITH_PETSc @OpenCMISS_WITH_PETSc@)
set(OpenCMISS_WITH_SLEPc @OpenCMISS_WITH_SLEPc@)
set(OpenCMISS_WITH_SUNDIALS @OpenCMISS_WITH_SUNDIALS@)

# Handle dependencies
include(CMakeFindDependencyMacro)
if(OpenCMISS_WITH_MPI)
  set(OpenCMISS_MPI_VERSION "@OpenCMISS_MPI_VERSION@")
  find_dependency(MPI "${OpenCMISS_MPI_VERSION}" REQUIRED)
endif()
if(OpenCMISS_WITH_OpenMP)
  set(OpenCMISS_OpenMP_VERSION "@OpenCMISS_OpenMP_VERSION@")
  find_dependency(OpenMP "${OpenCMISS_OpenMP_VERSION}" REQUIRED)
endif()
if(OpenCMISS_WITH_CUDA)
  set(OpenCMISS_CUDA_VERSION "@OpenCMISS_CUDA_VERSION@")
  find_dependency(CUDA "${OpenCMISS_CUDA_VERSION}" REQUIRED)
endif()

if(OpenCMISS_WITH_CellML)
  set(OpenCMISS_CellML_VERSION "@OpenCMISS_CellML_VERSION@")
  find_dependency(libCellML "${OpenCMISS_CellML_VERSION}" REQUIRED)
endif()
if(OpenCMISS_WITH_FieldML)
  set(OpenCMISS_FieldML_VERSION "@OpenCMISS_FieldML_VERSION@")
  find_dependency(FieldML "${OpenCMISS_FieldML_VERSION}" REQUIRED)
endif()
if(OpenCMISS_WITH_HYPRE)
  set(OpenCMISS_HYPRE_VERSION "@OpenCMISS_HYPRE_VERSION@")
  find_dependency(HYPRE "${OpenCMISS_HYPRE_VERSION}" REQUIRED)
endif()
if(OpenCMISS_WITH_MUMPS)
  set(OpenCMISS_MUMPS_VERSION "@OpenCMISS_MUMPS_VERSION@")
  find_dependency(MUMPS "${OpenCMISS_MUMPS_VERSION}" REQUIRED)
endif()
if(OpenCMISS_WITH_ParMETIS)
  set(OpenCMISS_ParMETIS_VERSION "@OpenCMISS_ParMETIS_VERSION@")
  find_dependency(ParMETIS "${OpenCMISS_ParMETIS_REQUIRED}" REQUIRED)
endif()
if(OpenCMISS_WITH_PETSc)
  set(OpenCMISS_PETSc_VERSION "@OpenCMISS_PETSc_VERSION@")
  find_dependency(PETSc "${OpenCMISS_PETSc_VERSION}" REQUIRED)
endif()
if(OpenCMISS_WITH_SLEPc)
  set(OpenCMISS_SLEPc_VERSION "@OpenCMISS_SLEPc_VERSION@")
  find_dependency(SLEPc "${OpenCMISS_SLEPc_VERSION}" REQUIRED)
endif()
if(OpenCMISS_WITH_SUNDIALS)
  set(OpenCMISS_SUNDIALS_VERSION "@OpenCMISS_SUNDIALS_VERSION@")
  find_dependency(SUNDIALS "${OpenCMISS_SUNDIALS_VERSION}" REQUIRED)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@-targets.cmake")
